          "blockClass": "footer__image--ventatelefonica",
          "link":{
              "url": "#",
              "newTab": false,
              "attributeNoFollow": false
          }
      }
  },
    "rich-text#footer__container--contacto-col-three":{
      "props":{
        "text": "Venta telefónica",
        "textPosition": "CENTER",
        "blockClass": "rich-text__footer__container--contacto-title"
      }
    },
    "rich-text#footer__container--serv-cliente-col-three":{
      "props":{
        "text": "Servicio al cliente: (601) 407 3033",
        "textPosition": "CENTER",
        "blockClass": "rich-text__footer__container--contacto"
      }
    },
    "rich-text#footer__container--vta-telefonica-col-three":{
      "props":{
        "text": "Venta Telefónica: (601) 3649734",
        "textPosition": "CENTER",
        "blockClass": "rich-text__footer__container--contacto"
      }
    }
     
